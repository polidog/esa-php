{
    "name": "polidog/esa-php",
    "description": "esa.io api library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryota Mochizuki",
            "email": "polidogs@gmail.com"
        }
    ],
    "require": {
      "php": ">=8.0",
      "ext-json": "*",
      "guzzlehttp/guzzle": "~7.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^9.6",
        "friendsofphp/php-cs-fixer": "^3.23",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
          "Polidog\\Esa\\": "src"
        }
    },
  "autoload-dev": {
    "psr-4": {
      "Polidog\\Esa\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "cs-fix": "./vendor/bin/php-cs-fixer fix"
  }
}
